<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:rendervm="clr-namespace:RenderModule.ViewModels;assembly=RenderModule"
                    xmlns:renderview="clr-namespace:RenderModule.Views;assembly=RenderModule">

    <Style x:Key="SidebarControlBorder" TargetType="Border">
        <Setter Property="BorderBrush" Value="Orchid" />
        <Setter Property="BorderThickness" Value="3" />
        <Setter Property="Background" Value="DarkBlue" />
        <Setter Property="Margin" Value="3" />
    </Style>

    <Style x:Key="ViewBorder" TargetType="Border">
        <Setter Property="BorderBrush" Value="Orchid" />
        <Setter Property="BorderThickness" Value="3" />
        <Setter Property="Background" Value="DarkBlue" />
        <Setter Property="Margin" Value="3" />
    </Style>

